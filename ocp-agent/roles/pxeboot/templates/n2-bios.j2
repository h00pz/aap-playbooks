default menu.c32
prompt 1
timeout 30

label coreos
  menu label {{ node2_name }} 	 
  menu default
  kernel {{ pxeboot_file }}/agent.x86_64-vmlinuz
  append initrd={{ pxeboot_file }}/agent.x86_64-initrd.img coreos.live.rootfs_url={{ pxeboot_http }}/agent.x86_64-rootfs.img random.trust_cpu=on rd.luks.options=discard ignition.firstboot ignition.platform.id=metal console=tty1 console=ttyS1,115200n8 coreos.inst.persistent-kargs="console=tty1 console=ttyS1,115200n8"